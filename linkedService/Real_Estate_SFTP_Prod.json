{
	"name": "Real_Estate_SFTP_Prod",
	"properties": {
		"parameters": {
			"SFTP_Hostname": {
				"type": "string",
				"defaultValue": "sftp.ashleyfurniture.com"
			},
			"SFTP_PortNumber": {
				"type": "int",
				"defaultValue": 22
			},
			"SFTP_Uname": {
				"type": "string",
				"defaultValue": "v_LeaseHarbor"
			}
		},
		"annotations": [],
		"type": "Sftp",
		"typeProperties": {
			"host": "@{linkedService().SFTP_Hostname}",
			"port": "22",
			"skipHostKeyValidation": true,
			"authenticationType": "Basic",
			"userName": "@{linkedService().SFTP_Uname}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AFIEDME_KeyVault_dev",
					"type": "LinkedServiceReference"
				},
				"secretName": "LeaseHarborPwd"
			}
		},
		"connectVia": {
			"referenceName": "SelfHosted-IR",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}