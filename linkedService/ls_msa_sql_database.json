{
	"name": "ls_msa_sql_database",
	"properties": {
		"parameters": {
			"ser_server_name": {
				"type": "String",
				"defaultValue": "afi-edme-semarchy-dev-sql.database.windows.net"
			},
			"database_name": {
				"type": "String",
				"defaultValue": "semarchy_staging"
			},
			"user_name": {
				"type": "String",
				"defaultValue": "NULL"
			},
			"db_pwd": {
				"type": "String",
				"defaultValue": "NULL"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"server": "@{linkedService().ser_server_name}",
			"database": "@{linkedService().database_name}",
			"encrypt": "mandatory",
			"trustServerCertificate": false,
			"authenticationType": "SystemAssignedManagedIdentity"
		}
	}
}