{
	"name": "TestRestPagination",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"QueryParameters.ContinuationToken": "$.$.ContinuationToken",
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "arrayOfObjects"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "RestResourceTest",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "JsonResponseDSTest",
						"type": "DatasetReference",
						"parameters": {
							"fileName": "@concat('Json_Output_Test_', convertFromUtc(utcnow(), 'Eastern Standard Time'))"
						}
					}
				]
			}
		],
		"annotations": []
	}
}