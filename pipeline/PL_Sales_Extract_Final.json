{
	"name": "PL_Sales_Extract_Final",
	"properties": {
		"activities": [
			{
				"name": "GetFileName",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "select top 1 FilesName from dbo.BatchProcess where Status is null order by ID",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "cdp_customer_deduped",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "Stored procedure1",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "SetFileName",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[dbo].[uspTransferSalesFromStgToWrk]"
				},
				"linkedServiceName": {
					"referenceName": "CDP_CustomerProfiling",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "GenerateJsonl",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Stored procedure1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT  (\r\n    SELECT DISTINCT \r\n\t\t   soh.[SourceSystemID]\r\n          ,soh.[OrderID]\r\n          ,soh.[CompanyCode]\r\n          ,soh.[CustomerName]\r\n          ,soh.[HasProtectionPlan]\r\n          ,soh.[OrderDate]\r\n          ,soh.[OrderStatus]\r\n          ,soh.[SalesChannel]\r\n          ,soh.[SalespersonID]\r\n          ,soh.[SourceCustomerID]\r\n          ,soh.[StoreID]\r\n          ,soh.[TotalValue]\r\n          ,soh.[BillToAddress]\r\n          ,soh.[BillToCity]\r\n          ,soh.[BillToStateProvinceCode]\r\n          ,soh.[BillToPostalCode]\r\n          ,soh.[BillToCountryCode]\r\n          ,soh.[Phone1]\r\n          ,soh.[Phone2]\r\n          ,soh.[Phone3]\r\n          ,EmailAddress = soh.[Email]\r\n          ,(SELECT DISTINCT\r\n              sol.[OrderLineNumber]\r\n              ,sol.[DeliveryDate]\r\n              ,sol.[DeliveryType]\r\n              ,sol.[InvoiceDate]\r\n              ,sol.[IsDeliveryScheduled]\r\n              ,sol.[ItemDescription]\r\n              ,sol.[ItemTotal]\r\n              ,sol.[OrderQuantity]\r\n              ,sol.[SKU]\r\n              ,sol.[VendorID]\r\n              ,sol.[ShipToAddress]\r\n              ,sol.[ShipToAddress2]\r\n              ,sol.[ShipToCity]\r\n              ,sol.[ShipToStateProvinceCode]\r\n              ,sol.[ShipToPostalCode]\r\n              ,sol.[ShipToCountryCode],\r\n\t\t\t  (select sod.DiscountAmount,sod.DiscountCode from [dbo].[SalesOrderLineDiscount_Wrk]  sod  with(nolock) where sod.SourceSystemID = sol.SourceSystemID and sod.OrderID = sol.OrderID and sod.OrderLineNumber = sol.OrderLineNumber FOR JSON PATH) AS Discounts\r\n          FROM [dbo].[SalesOrderLine_Wrk] sol WHERE soh.SourceSystemID = sol.SourceSystemID AND soh.OrderID = sol.OrderID ORDER BY sol.OrderLineNumber FOR JSON PATH) AS OrderLines,\r\n            (select sot.AmountTendered,sot.TenderType from [dbo].[SalesOrderTender_Wrk] sot with(nolock) where soh.SourceSystemID = sot.SourceSystemID and soh.OrderID = sot.OrderID FOR JSON PATH) AS Tenders\r\n        FOR JSON PATH , WITHOUT_ARRAY_WRAPPER\r\n    ) as jsonl \r\nFROM [dbo].[SalesOrderHeader_Wrk] soh \r\n    INNER JOIN [dbo].[SalesOrderHeader_Wrk] sohMap ON soh.SourceSystemID = sohMap.SourceSystemID AND soh.OrderID = sohMap.OrderID AND soh.SourceCustomerID = sohMap.SourceCustomerID\r\nORDER BY soh.SourceSystemID, soh.OrderDate, soh.OrderID, soh.SourceCustomerID",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".jsonl"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "cdp_customer_deduped",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SalesOrder",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@variables('FileName')",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "SetFileName",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "GetFileName",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "FileName",
					"value": {
						"value": "@{activity('GetFileName').output.firstRow.FilesName}",
						"type": "Expression"
					}
				}
			},
			{
				"name": "UpdateStatusa",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Get_count_no_line",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "CDP_CustomerProfiling",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": {
								"value": "Update bp set status = 'Complete',\nNoOrderLine='@{activity('Get_count_no_line').output.firstRow.NoOrderLine}' \nfrom dbo.BatchProcess bp with(nolock) where \nFilesName='@{variables('FileName')}'",
								"type": "Expression"
							}
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "SalesOrderError",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "Stored procedure1",
						"dependencyConditions": [
							"Failed"
						]
					},
					{
						"activity": "GenerateJsonl",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[Audit].[uspInsertADFErrorLog]",
					"storedProcedureParameters": {
						"ErrorCode": {
							"value": {
								"value": "@{activity('GenerateJsonl').error.errorCode}",
								"type": "Expression"
							},
							"type": "String"
						},
						"ErrorMsg": {
							"value": {
								"value": "@{activity('GenerateJsonl').error.message}",
								"type": "Expression"
							},
							"type": "String"
						},
						"ErrorStatusCode": {
							"value": {
								"value": "@activity('GenerateJsonl').StatusCode",
								"type": "Expression"
							},
							"type": "String"
						},
						"FailureType": {
							"value": {
								"value": "@{activity('GenerateJsonl').error.failureType}\n",
								"type": "Expression"
							},
							"type": "String"
						},
						"PipelineName": {
							"value": {
								"value": "@{pipeline().Pipeline}",
								"type": "Expression"
							},
							"type": "String"
						},
						"RunID": {
							"value": {
								"value": "@{pipeline().RunId}",
								"type": "Expression"
							},
							"type": "String"
						},
						"Sink": {
							"value": {
								"value": "SalesOrder",
								"type": "Expression"
							},
							"type": "String"
						},
						"Sink_Type": {
							"value": {
								"value": "@{activity('GenerateJsonl').output.executionDetails[0].sink.type}",
								"type": "Expression"
							},
							"type": "String"
						},
						"Source": {
							"value": {
								"value": "cdp_customer_deduped",
								"type": "Expression"
							},
							"type": "String"
						},
						"Source_Type": {
							"value": {
								"value": "@{activity('GenerateJsonl').output.executionDetails[0].source.type}",
								"type": "Expression"
							},
							"type": "String"
						},
						"TriggerId": {
							"value": {
								"value": "@{pipeline().TriggerId}",
								"type": "Expression"
							},
							"type": "String"
						},
						"TriggerName": {
							"value": {
								"value": "@{pipeline().TriggerName}",
								"type": "Expression"
							},
							"type": "String"
						},
						"TriggerTime": {
							"value": {
								"value": "@{pipeline().TriggerTime}",
								"type": "Expression"
							},
							"type": "String"
						},
						"TriggerType": {
							"value": {
								"value": "@{pipeline().TriggerType}",
								"type": "Expression"
							},
							"type": "String"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "MarketingHub",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Get_count_no_line",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "GenerateJsonl",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "select count(*) as NoOrderLine from (\nselect distinct sohs.SourceSystemID,sohs.SourceCustomerID,sohs.OrderID from [dbo].[SalesOrderHeader_Wrk] sohs \nwhere sohs.OrderID not in (select distinct sohs.OrderID from [dbo].[SalesOrderLine_Wrk] sohs))temp",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "cdp_customer_deduped",
						"type": "DatasetReference"
					}
				}
			}
		],
		"variables": {
			"FileName": {
				"type": "String"
			}
		},
		"annotations": []
	}
}