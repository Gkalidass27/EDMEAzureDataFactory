				"dependsOn": [
					{
						"activity": "Stored procedure1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "select  \r\n(select soh.*,\r\n(select sol.*, \r\n\t(select * from [dbo].[SalesOrderLineDiscount_Wrk]  sod  with(nolock) where sod.SourceSystemID = sol.SourceSystemID and sod.OrderID = sol.OrderID and sod.OrderLineNumber = sol.OrderLineNumber FOR JSON PATH) AS Discounts\r\nfrom [dbo].[SalesOrderLine_Wrk] sol with(nolock) where soh.SourceSystemID = sol.SourceSystemID and soh.OrderID = sol.OrderID FOR JSON PATH) AS OrderLines,\r\n(select sot.* from [dbo].[SalesOrderTender_Wrk] sot with(nolock) where soh.SourceSystemID = sot.SourceSystemID and soh.OrderID = sot.OrderID FOR JSON PATH) AS Tenders\r\nFOR JSON PATH , WITHOUT_ARRAY_WRAPPER) as jsonl  from\r\n [dbo].[SalesOrderHeader_Wrk]  soh with(nolock)\r\ninner join [dbo].[SalesOrderHeader_Wrk] sohMap with(nolock) on soh.SourceSystemID = sohMap.SourceSystemID and soh.OrderID = sohMap.OrderID and soh.CustomerName = sohMap.CustomerName",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".jsonl"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Standard_MarketingHub_Dataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SalesOrder",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@variables('FileName')",
								"type": "Expression"
							}
						}
					}
				]
			},
