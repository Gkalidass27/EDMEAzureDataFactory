				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "SELECT SUBSTRING('@{variables('File_Name')}', CHARINDEX('_', '@{variables('File_Name')}') + 1, CHARINDEX('.','@{variables('File_Name')}') - CHARINDEX('_', '@{variables('File_Name')}')-1) as FinalVar;\n",
