					{
						"activity": "Get_Last_Trigger_Time",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "ToDateTime",
					"value": {
						"value": "@utcNow()",
						"type": "Expression"
					}
				}
			},
