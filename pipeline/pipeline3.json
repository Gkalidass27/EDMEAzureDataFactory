{
	"name": "pipeline3",
	"properties": {
		"activities": [
			{
				"name": "GetFileList",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": {
						"value": "@concat('/releases/data/z5max.txt','?FORMAT=JSON&ID=',pipeline().parameters.ID)",
						"type": "Expression"
					},
					"linkedServiceName": {
						"referenceName": "HttpLinkedService",
						"type": "LinkedServiceReference"
					}
				}
			},
			{
				"name": "SetCurrentMonthYear",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "GetFileList",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "currentMonthYear",
					"value": {
						"value": "@concat(formatDateTime(utcNow(), 'yyyy.MM'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "FilterCurrentMonthYear",
				"type": "Filter",
				"dependsOn": [
					{
						"activity": "SetCurrentMonthYear",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('GetFileList').output.value",
						"type": "Expression"
					},
					"condition": {
						"value": "@equals(item(), variables('currentMonthYear'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "IfCurrentMonthYearExists",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "FilterCurrentMonthYear",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@greater(length(activity('FilterCurrentMonthYear').output.value), 0)",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "Set variable1",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "downloadUrl",
								"value": "'False'"
							}
						}
					],
					"ifTrueActivities": [
						{
							"name": "SetDownloadURL",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "downloadUrl",
								"value": {
									"value": "'True'",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"ID": {
				"type": "string",
				"defaultValue": "ZJERTAWalLbr19V1DoknD4**"
			}
		},
		"variables": {
			"currentMonthYear": {
				"type": "String"
			},
			"downloadUrl": {
				"type": "String"
			}
		},
		"annotations": []
	}
}