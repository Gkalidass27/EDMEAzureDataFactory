{
	"name": "MSA_Master_Loading_To_External_Source",
	"properties": {
		"activities": [
			{
				"name": "Lookup_Server_Metadata",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "SELECT\n      server_id\n       ,server_name\n      ,ser_database_name\n      ,ser_schema_name\n      ,tab_name\n      ,ser_user_name\n      ,ser_pwd\n      ,isactive\n       from Ref_PostalCode.MSATargetServerConfig",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "Semarchy_Staging",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Iteration_servers",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Lookup_Server_Metadata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Lookup_Server_Metadata').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy_MSA_Masterdata_External_source",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"sqlReaderQuery": {
										"value": "SELECT MSA_MASTER_ID ,MSA_DESCRIPTION\n  FROM [dbo].[GD_MSA_MASTER]\n",
										"type": "Expression"
									},
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "AzureSqlSink",
									"preCopyScript": {
										"value": "@concat('DELETE FROM ',item().ser_schema_name, '.', item().tab_name)\n",
										"type": "Expression"
									},
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "GD_MSA_MASTER_Source",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "ds_msamaster",
									"type": "DatasetReference",
									"parameters": {
										"schema_name": {
											"value": "@item().ser_schema_name",
											"type": "Expression"
										},
										"table_name": {
											"value": "@item().tab_name",
											"type": "Expression"
										},
										"ser_server_name": {
											"value": "@item().server_name",
											"type": "Expression"
										},
										"database_name": {
											"value": "@item().ser_database_name",
											"type": "Expression"
										},
										"user_name": {
											"value": "@item().ser_user_name",
											"type": "Expression"
										},
										"db_pwd": {
											"value": "@item().ser_pwd",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "Fail_Load_PostalCode",
							"type": "Fail",
							"dependsOn": [
								{
									"activity": "Teams_Failed_Notification_PostalCode",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"message": {
									"value": "@replace(activity('Copy_MSA_Masterdata_External_source').error.message,'\"','')",
									"type": "Expression"
								},
								"errorCode": {
									"value": "@{activity('Copy_MSA_Masterdata_External_source').error.errorCode}",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Teams_Failed_Notification_PostalCode",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "Copy_MSA_Masterdata_External_source",
									"dependencyConditions": [
										"Failed"
									]
								}
							],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "Notify_Teams_Channale_Pipeline",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"subscription": {
										"value": "@pipeline().globalParameters.ADF_SubscriptionID",
										"type": "Expression"
									},
									"resourceGroup": "afi-edme-rg",
									"runId": {
										"value": "@pipeline().RunId",
										"type": "Expression"
									},
									"name": {
										"value": "@pipeline().Pipeline",
										"type": "Expression"
									},
									"triggerTime": {
										"value": "@activity('Copy_MSA_Masterdata_External_source').output.executionDetails[0].start",
										"type": "Expression"
									},
									"status": {
										"value": "@activity('Copy_MSA_Masterdata_External_source').output.executionDetails[0].status",
										"type": "Expression"
									},
									"message": {
										"value": "@replace(activity('Copy_MSA_Masterdata_External_source').error.message,'\"','')",
										"type": "Expression"
									},
									"executionEndTime": {
										"value": "@{utcnow()}",
										"type": "Expression"
									},
									"runDuration": {
										"value": "@activity('Copy_MSA_Masterdata_External_source').output.copyDuration",
										"type": "Expression"
									},
									"teamWebhookUrl": {
										"value": "https://masterashley.webhook.office.com/webhookb2/c31879ef-6dfb-470f-b788-de8363ae3c17@5a9d9cfd-c32e-4ac1-a9ed-fe83df4f9e4d/IncomingWebhook/147e6de5d4454f53bc64a6a9bae90283/276c88be-8000-43f8-b798-97215f5a2fda",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"server_name": {
				"type": "string"
			},
			"ser_database_name": {
				"type": "string"
			},
			"ser_schema_name": {
				"type": "string"
			},
			"tab_name": {
				"type": "string"
			},
			"ser_user_name": {
				"type": "string"
			},
			"ser_pwd": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Postal_Code"
		},
		"annotations": []
	}
}