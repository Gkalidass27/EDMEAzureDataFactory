{
	"name": "Stage_EDW_DS_Tracking_No",
	"properties": {
		"description": "Load ds tracking from edw to cosmosdb",
		"activities": [
			{
				"name": "Stage_EDW_DS_Tracking",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "upsert",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"dataIntegrationUnits": 2,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "OrderNumber",
									"type": "String"
								},
								"sink": {
									"path": "$['OrderNumber']"
								}
							},
							{
								"source": {
									"name": "CustomerNumber",
									"type": "String"
								},
								"sink": {
									"path": "$['CustomerNumber']"
								}
							},
							{
								"source": {
									"name": "InvoiceNumber",
									"type": "String"
								},
								"sink": {
									"path": "$['InvoiceNumber']"
								}
							},
							{
								"source": {
									"name": "InvoiceDate",
									"type": "String"
								},
								"sink": {
									"path": "$['InvoiceDate']"
								}
							},
							{
								"source": {
									"name": "ItemNumber",
									"type": "String"
								},
								"sink": {
									"path": "$['ItemNumber']"
								}
							},
							{
								"source": {
									"name": "SerialNumber",
									"type": "String"
								},
								"sink": {
									"path": "$['SerialNumber']"
								}
							},
							{
								"source": {
									"name": "Carrier",
									"type": "String"
								},
								"sink": {
									"path": "$['Carrier']"
								}
							},
							{
								"source": {
									"name": "TrackingNumber",
									"type": "String"
								},
								"sink": {
									"path": "$['TrackingNumber']"
								}
							},
							{
								"source": {
									"name": "ItemSequence",
									"type": "String"
								},
								"sink": {
									"path": "$['id']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "dstrackingsample",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "dstrackingcosmos",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Data Fabric"
		},
		"annotations": []
	}
}