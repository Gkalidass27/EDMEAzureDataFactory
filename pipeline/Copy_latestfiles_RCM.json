{
	"name": "Copy_latestfiles_RCM",
	"properties": {
		"activities": [
			{
				"name": "Get Metadata_Subfolder",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "Error_folder",
						"type": "DatasetReference",
						"parameters": {
							"Foldername": {
								"value": "@pipeline().parameters.PL_Foldername",
								"type": "Expression"
							},
							"path": {
								"value": "@pipeline().parameters.PL_Path",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "BinaryReadSettings"
					}
				}
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Get Metadata_Subfolder",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Get Metadata_Subfolder').output.childItems",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Execute Pipeline1",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "Notify_error_RCM_Files",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"Path": {
										"value": "@pipeline().parameters.PL_Path",
										"type": "Expression"
									},
									"Foldername": {
										"value": "@pipeline().parameters.PL_Foldername",
										"type": "Expression"
									},
									"Subfoldername": {
										"value": "@item().name",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"PL_Path": {
				"type": "string",
				"defaultValue": "Error/copyactivity-logs/"
			},
			"PL_Foldername": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Retail_Customer_Flow"
		},
		"annotations": []
	}
}