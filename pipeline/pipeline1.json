{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy Excel to Archive with Timestamp",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": false,
							"wildcardFolderPath": "RealEstate_Staging/SOURCE",
							"wildcardFileName": "*",
							"deleteFilesAfterCompletion": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Real_estate_BinaryFiles",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Archive_real_Estate",
						"type": "DatasetReference",
						"parameters": {
							"fileName": "archived_file.xlsx"
						}
					}
				]
			}
		],
		"folder": {
			"name": "SiteMaster"
		},
		"annotations": []
	}
}