{
	"name": "Load_DAC_Json_Data_To_SQL",
	"properties": {
		"activities": [
			{
				"name": "GetAllJsonFiles",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "GetMetaDataJsonPayLoad",
						"type": "DatasetReference"
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "JsonReadSettings"
					}
				}
			},
			{
				"name": "ForEachJsonFile",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "TruncateStoresTempTable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('GetAllJsonFiles').output.childItems",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "LookupJsonFile",
							"type": "Lookup",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "JsonSource",
									"storeSettings": {
										"type": "AzureBlobFSReadSettings",
										"recursive": true,
										"enablePartitionDiscovery": false
									},
									"formatSettings": {
										"type": "JsonReadSettings"
									}
								},
								"dataset": {
									"referenceName": "JsonPayload",
									"type": "DatasetReference",
									"parameters": {
										"fileName": {
											"value": "@item().name",
											"type": "Expression"
										}
									}
								}
							}
						},
						{
							"name": "ExecuteLoadStoreDetailsPipeline",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "LookupJsonFile",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "Load_Store_Details",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"item_param": {
										"value": "@activity('LookupJsonFile').output.firstRow.Data",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			},
			{
				"name": "StoreDataIntoDACTables",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "ForEachJsonFile",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[DAC].[Load_Stores_Data]"
				},
				"linkedServiceName": {
					"referenceName": "semarchy_dev_staging",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "TruncateStoresTempTable",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "GetAllJsonFiles",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[DAC].[Truncate_Stores_Temp]"
				},
				"linkedServiceName": {
					"referenceName": "semarchy_dev_staging",
					"type": "LinkedServiceReference"
				}
			}
		],
		"variables": {
			"location_id_var": {
				"type": "String"
			}
		},
		"annotations": []
	}
}