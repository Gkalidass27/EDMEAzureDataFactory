{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Canada_Postal_Code_File_melissa",
						"type": "DatasetReference"
					},
					"name": "FixedWidthData"
				},
				{
					"dataset": {
						"referenceName": "Canada_Postal_Code_File_melissa_overlay",
						"type": "DatasetReference"
					},
					"name": "source1",
					"description": "Import data from Canada_Postal_Code_File_melissa overley"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CANADA_Postal_Code_Table_Melissa",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "Canada_Postal_Code_File_melissa_overlay",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {_col0_} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> FixedWidthData",
				"source(output(",
				"          {_col0_} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> source1",
				"FixedWidthData derive(PostalCode = substring({_col0_},1,6),",
				"          City = substring({_col0_}, 7, 30),",
				"          Province = substring({_col0_}, 37,2),",
				"          Latitude = substring({_col0_}, 39, 9),",
				"          Longitude = substring({_col0_}, 48, 11),",
				"          TimeZone = substring({_col0_}, 59, 2),",
				"          DST = substring({_col0_}, 61, 1),",
				"          AreaCode = substring({_col0_}, 62, 3),",
				"          Overlay = substring({_col0_}, 65, 1)) ~> derivedColumn1",
				"derivedColumn1 select(skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> select1",
				"source1 derive(Parent = substring({_col0_},1,3),",
				"          Overlay = substring({_col0_},4,3),",
				"          Group = substring({_col0_},5,1)) ~> derivedColumn2",
				"derivedColumn2 select(mapColumn(",
				"          Parent,",
				"          Overlay,",
				"          Group",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> select2",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink2"
			]
		}
	}
}