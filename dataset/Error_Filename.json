{
	"name": "Error_Filename",
	"properties": {
		"linkedServiceName": {
			"referenceName": "masterDataHubStg",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Path": {
				"type": "string"
			},
			"Foldername": {
				"type": "string"
			},
			"Subfoldername": {
				"type": "string"
			},
			"Filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().Filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().Path,dataset().Foldername,'/',dataset().Subfoldername)",
					"type": "Expression"
				},
				"fileSystem": "retail-customer"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}